    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#858C99"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#ff63a7",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords ",
            "scope": [
                "keyword",
                "keyword.operator.expression",
                "storage.modifier",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#888BF4",
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword <new> | Keyword <default> ",
            "scope": [
                "keyword.operator.new",
                "keyword.control.default"
            ],
            "settings": {
                "foreground": "#63ffe3",
                "fontStyle": ""
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "keyword.operator.assignment",
                "storage.type.function.arrow",
                "keyword.operator.spread",
                "keyword.operator.rest"
            ],
            "settings": {
                "foreground": "#fdff6a",
                "fontStyle": ""
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "support.type.builtin"
            ],
            "settings": {
                "foreground": "#66e7ff",
                "fontStyle": ""
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#BDC0C7",
                "fontStyle": ""
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#92bef5",
                "fontStyle": ""
            }
        },
        {
            "name": "Storage Type",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#8bf488",
                "fontStyle": ""
            }
        },
        {
            "name": "Primitives",
            "scope": [
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#fcb100",
                "fontStyle": ""
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#ffd268",
                "fontStyle": ""
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#FC033D"
            }
        },
        {
            "name": "Tag Arrow",
            "scope": [
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#bb0e0e"
            }
        },
        {
            "name": "Component",
            "scope": [
                "support.class.component"
            ],
            "settings": {
                "foreground": "#FD547B"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#AEAFF8"
            }
        },
        {
            "name": "CSS Classes",
            "scope": [
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#63ffe3"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#ffa7c1"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "#ffa7c1"
    }
}