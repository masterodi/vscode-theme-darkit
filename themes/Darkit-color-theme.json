    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#9a9da3"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#8eebec"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#ffa562"
            }
        },
        {
            "name": "Keywords | Storage Type (let, const, var) | async",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#ff4a7e"
            }
        },
        {
            "name": "Operators | this | super | self",
            "scope": [
                "keyword.operator",
                "variable.language.this",
                "variable.language.super",
                "variable.language.special.self"
            ],
            "settings": {
                "foreground": "#58ffb7"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#79c3ff"
            }
        },
        {
            "name": "Classes  ",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#ffc37e"
            }
        },
        {
            "name": "Variable properties",
            "scope": [
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#aea5ff"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#ffff63"
            }
        },
        {
            "name": "Components",
            "scope": [
                "support.class.component"
            ],
            "settings": {
                "foreground": "#ffffad"
            }
        },
        {
            "name": "Tag brackets",
            "scope": [
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#ffc37e"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffc3ff"
            }
        },
        {
            "name": "Css Classes",
            "scope": [
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#b5ffff"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#7dffd5"
            }
        },
        {
            "name": "Units",
            "scope": [
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#fff1aa"
            }
        },
        {
            "name": "Dictionary key",
            "scope": [
                "support.type",
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#ff4a7e"
            }
        }
    ]
}