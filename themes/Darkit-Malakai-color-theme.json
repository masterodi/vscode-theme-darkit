    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#7680A4"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#f1f5f9"
            }
        },
        {
            "name": "Variable other property",
            "scope": [
                "support.variable",
                "variable.other.predefined",
                "variable.other.property",
                "meta.object.member"
            ],
            "settings": {
                "foreground": "#D9F3FC"
            }
        },
        {
            "name": "Variable other readwrite alias",
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#c4b5fd"
            }
        },
        {
            "name": "Variable other constant object",
            "scope": [
                "variable.other.constant.object"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call.generic",
                "support.function.magic",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#818cf8"
            }
        },
        {
            "name": "Function parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#c7d2fe",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#FFFC99"
            }
        },
        {
            "name": "String regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#FFF94D",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Primitives",
            "scope": [
                "support.type.primitive",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "support.constant",
                "constant.language",
                "support.type.builtin"
            ],
            "settings": {
                "foreground": "#29F4FF"
            }
        },
        {
            "name": "Numeric constant",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#29F4FF"
            }
        },
        {
            "name": "Keyword operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "punctuation.accessor",
                "punctuation.separator.key-value",
                "punctuation.definition.block.sequence.item",
                "punctuation.separator.dictionary.key-value",
                "punctuation.definition.variable",
                "punctuation.separator.colon",
                "punctuation.separator.period",
                "punctuation.section",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "Keywords | Keyword operator",
            "scope": [
                "keyword",
                "meta.method.declaration storage.type"
            ],
            "settings": {
                "foreground": "#33F4B7"
            }
        },
        {
            "name": "Storage (let, const, async, function, class, extends, ...) | Variable language | Keyword operator expression | Keyword operator new",
            "scope": [
                "storage",
                "variable.language",
                "keyword.operator.expression",
                "keyword.operator.new",
                "keyword.function"
            ],
            "settings": {
                "foreground": "#33F4B7"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "support.type"
            ],
            "settings": {
                "foreground": "#F92672"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self",
                "variable.language.special.self",
                "entity.name.tag.reference"
            ],
            "settings": {
                "foreground": "#F92672"
            }
        },
        {
            "name": "Punctuation tag",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#951744"
            }
        },
        {
            "name": "HTML Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#FB679C",
                "fontStyle": "italic"
            }
        },
        {
            "name": "HTML Ids",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#fb923c"
            }
        },
        {
            "name": "Json key, Yaml key",
            "scope": [
                "support.type.property-name.json",
                "source.yaml entity.name.tag",
                "source.css support.type.property-name"
            ],
            "settings": {
                "foreground": "#2EEDBA"
            }
        }
    ]
}