    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#475569"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e2e8f0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call.generic",
                "variable.other.constant.object.ts",
                "variable.other.constant.object.tsx",
                "support.function.magic"
            ],
            "settings": {
                "foreground": "#8EDAF5"
            }
        },
        {
            "name": "Function parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords | Keyword operator",
            "scope": [
                "keyword",
                "meta.method.declaration storage.type"
            ],
            "settings": {
                "foreground": "#7AAFFF"
            }
        },
        {
            "name": "Storage (let, const, async, function, class, extends, ...) | Variable language | Keyword operator expression | Keyword operator new",
            "scope": [
                "storage",
                "variable.language",
                "keyword.operator.expression",
                "keyword.operator.new",
                "keyword.function"
            ],
            "settings": {
                "foreground": "#7AAFFF"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "support.type"
            ],
            "settings": {
                "foreground": "#c4b5fd"
            }
        },
        {
            "name": "Primitives",
            "scope": [
                "support.type.primitive",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#818cf8"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#68F7CA"
            }
        },
        {
            "name": "String regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#a5b4fc",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self",
                "variable.language.special.self",
                "entity.name.tag.reference"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "Json key, Yaml key",
            "scope": [
                "support.type.property-name.json",
                "source.yaml entity.name.tag"
            ],
            "settings": {
                "foreground": "#90ACD0"
            }
        },
        {
            "name": "Punctuation tag",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#94a3b8"
            }
        },
        {
            "name": "Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#90ACD0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Ids",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#35D7C7"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#FF527D"
            }
        },
        {
            "name": "Numeric constant",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FF527D"
            }
        },
        {
            "name": "Keyword operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "punctuation.accessor",
                "punctuation.separator.key-value",
                "punctuation.definition.block.sequence.item",
                "punctuation.separator.dictionary.key-value",
                "punctuation.definition.variable",
                "punctuation.separator.colon",
                "punctuation.separator.period",
                "punctuation.section",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#B5FBE5"
            }
        },
        {
            "name": "CSS property name",
            "scope": [
                "source.css support.type.property-name"
            ],
            "settings": {
                "foreground": "#5781B7"
            }
        },
        {
            "name": "Variable other property",
            "scope": [
                "support.variable",
                "variable.other.predefined",
                "variable.other.property",
                "meta.object.member"
            ],
            "settings": {
                "foreground": "#C9D7E8"
            }
        },
        {
            "name": "Variable other readwrite alias",
            "scope": [
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#ADC1DC"
            }
        }
    ]
}