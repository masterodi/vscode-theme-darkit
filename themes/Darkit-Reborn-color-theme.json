    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#777a80"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#e4e7ee"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call.generic"
            ],
            "settings": {
                "foreground": "#2dd4bf"
            }
        },
        {
            "name": "Keywords | Keyword operator",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#818cf8"
            }
        },
        {
            "name": "Storage (let, const, async, function, class, extends, ...) | Variable language | Keyword operator expression | Keyword operator new",
            "scope": [
                "storage",
                "variable.language",
                "keyword.operator.expression",
                "keyword.operator.new",
                "variable.parameter.function.language.special.self",
                "keyword.function"
            ],
            "settings": {
                "foreground": "#60a5fa"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "support.type.primitive",
                "entity.name.scope-resolution",
                "support.type"
            ],
            "settings": {
                "foreground": "#ff487d"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#ffa361"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#FF5874"
            }
        },
        {
            "name": "Punctuation tag",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#999ca2"
            }
        },
        {
            "name": "Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#a5b4fc"
            }
        },
        {
            "name": "Ids",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#a5b4fc"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#fcd34d"
            }
        },
        {
            "name": "Numeric constant",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#6AF699"
            }
        },
        {
            "name": "Keyword operator",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#a5b4fc"
            }
        },
        {
            "name": "Keyword operator assignment",
            "scope": [
                "keyword.operator.assignment",
                "punctuation.accessor",
                "punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#ff6c9a"
            }
        },
        {
            "name": "CSS property name",
            "scope": [
                "source.css support.type.property-name",
                "meta.property-list entity.name.tag"
            ],
            "settings": {
                "foreground": "#67e8f9"
            }
        }
    ]
}