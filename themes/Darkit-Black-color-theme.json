    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#7680A4"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#f1f5f9"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call.generic",
                "support.function.magic",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "name": "Function parameter",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#CCE0FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keywords | Keyword operator",
            "scope": [
                "keyword",
                "meta.method.declaration storage.type"
            ],
            "settings": {
                "foreground": "#A3C8FF"
            }
        },
        {
            "name": "Storage (let, const, async, function, class, extends, ...) | Variable language | Keyword operator expression | Keyword operator new",
            "scope": [
                "storage",
                "variable.language",
                "keyword.operator.expression",
                "keyword.operator.new",
                "keyword.function"
            ],
            "settings": {
                "foreground": "#A3C8FF"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "support.type"
            ],
            "settings": {
                "foreground": "#8FF9D8"
            }
        },
        {
            "name": "Primitives",
            "scope": [
                "support.type.primitive",
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element"
            ],
            "settings": {
                "foreground": "#F9A4E8"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#818cf8"
            }
        },
        {
            "name": "String regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#a5b4fc",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "variable.language.this",
                "variable.language.super",
                "variable.parameter.function.language.special.self",
                "variable.language.special.self",
                "entity.name.tag.reference"
            ],
            "settings": {
                "foreground": "#6ee7b7"
            }
        },
        {
            "name": "Json key, Yaml key",
            "scope": [
                "support.type.property-name.json",
                "source.yaml entity.name.tag"
            ],
            "settings": {
                "foreground": "#FF7A9C"
            }
        },
        {
            "name": "Punctuation tag",
            "scope": [
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag.end",
                "punctuation.definition.typeparameters.begin",
                "punctuation.definition.typeparameters.end"
            ],
            "settings": {
                "foreground": "#57DED0"
            }
        },
        {
            "name": "Attribute name",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#a5b4fc",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Ids",
            "scope": [
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#35D7C7"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "support.constant",
                "constant.language",
                "support.type.builtin"
            ],
            "settings": {
                "foreground": "#29F4FF"
            }
        },
        {
            "name": "Numeric constant",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#29F4FF"
            }
        },
        {
            "name": "Keyword operator",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "punctuation.accessor",
                "punctuation.separator.key-value",
                "punctuation.definition.block.sequence.item",
                "punctuation.separator.dictionary.key-value",
                "punctuation.definition.variable",
                "punctuation.separator.colon",
                "punctuation.separator.period",
                "punctuation.section",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#A3FAFF"
            }
        },
        {
            "name": "CSS property name",
            "scope": [
                "source.css support.type.property-name"
            ],
            "settings": {
                "foreground": "#ff75b6"
            }
        },
        {
            "name": "Variable other property",
            "scope": [
                "support.variable",
                "variable.other.predefined",
                "variable.other.property",
                "meta.object.member"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        },
        {
            "name": "Variable other readwrite alias",
            "scope": [
                "variable.other.readwrite.alias",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#B3E6F8"
            }
        },
        {
            "name": "Variable other constant object",
            "scope": [
                "variable.other.constant.object"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        }
    ],
    "semanticHighlighting": true
}