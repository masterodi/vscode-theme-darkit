{
	"name": "darkit",
	"displayName": "Darkit",
	"description": "A dark theme.",
	"type": "module",
	"publisher": "mrheio",
	"version": "3.8.0",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.7.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Darkit",
				"uiTheme": "vs-dark",
				"path": "./themes/Darkit-color-theme.json"
			},
			{
				"label": "Darkit Yami",
				"uiTheme": "vs-dark",
				"path": "./themes/Darkit-Yami-color-theme.json"
			}
		]
	},
	"scripts": {
		"dev": "nodemon ./src/build.js",
		"build": "node ./src/build.js"
	},
	"nodemonConfig": {
		"ignore": [
			"themes/*"
		]
	},
	"dependencies": {
		"color": "^4.2.3"
	},
	"devDependencies": {
		"nodemon": "^2.0.19"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mrheio/darkit"
	}
}
