{
	"name": "darkit",
	"displayName": "Darkit",
	"description": "A dark theme.",
	"type": "module",
	"publisher": "mrheio",
	"version": "26.0.0",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.7.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Darkit",
				"uiTheme": "vs-dark",
				"path": "./themes/Darkit.json"
			},
			{
				"label": "Darkit Astral",
				"uiTheme": "vs-dark",
				"path": "./themes/Darkit-Astral.json"
			}
		]
	},
	"scripts": {
		"dev": "node --watch-path=./src src/index.js",
		"build": "node ./src/index.js",
		"prepare": "husky install",
		"lint:fix": "eslint --fix"
	},
	"devDependencies": {
		"color": "^4.2.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"prettier": "2.8.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mrheio/vsc-theme-darkit"
	},
	"lint-staged": {
		"*.{js,css,md,json}": "prettier --write"
	}
}
