    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "{{ colors.neutral-400 }}"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "{{ colors.string }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords ",
            "scope": [
                "keyword",
                "keyword.operator.expression",
                "storage.modifier",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "{{ colors.keyword }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword <new> | Keyword <default> ",
            "scope": ["keyword.operator.new", "keyword.control.default"],
            "settings": {
                "foreground": "{{ colors.keywordAccent }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "keyword.operator.assignment",
                "storage.type.function.arrow",
                "keyword.operator.spread",
                "keyword.operator.rest"
            ],
            "settings": {
                "foreground": "{{ colors.operator }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Constants",
            "scope": ["constant", "support.type.builtin"],
            "settings": {
                "foreground": "{{ colors.constant }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Variables",
            "scope": ["variable"],
            "settings": {
                "foreground": "{{ colors.variable }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "{{ colors.function }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Storage Type",
            "scope": ["storage.type"],
            "settings": {
                "foreground": "{{ colors.storageType }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Primitives",
            "scope": ["support.type.primitive"],
            "settings": {
                "foreground": "{{ colors.primitive}}",
                "fontStyle": ""
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type"],
            "settings": {
                "foreground": "{{ colors.class }}",
                "fontStyle": ""
            }
        },
        {
            "name": "Tag",
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "{{ colors.tag }}"
            }
        },
        {
            "name": "Tag Arrow",
            "scope": ["punctuation.definition.tag"],
            "settings": {
                "foreground": "{{ colors.tagArrow}}"
            }
        },
        {
            "name": "Component",
            "scope": ["support.class.component"],
            "settings": {
                "foreground": "{{ colors.component }}"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "{{ colors.htmlAttribute }}"
            }
        },
        {
            "name": "CSS Classes",
            "scope": ["entity.other.attribute-name.class"],
            "settings": {
                "foreground": "{{ colors.cssClass }}"
            }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "{{ colors.cssProperty }}"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": "{{ colors.objectKey }}"
    }
}
